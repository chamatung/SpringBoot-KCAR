<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.one.kcar.repository.IStoreDAO">
	
	<select id="storelistAll" resultType="StoreDTO">
		select * from kcar_store
	</select>

	<select id="storelist"  parameterType="String" resultType="StoreDTO">
		select * from kcar_store where st_name = #{name}
	</select>

</mapper>