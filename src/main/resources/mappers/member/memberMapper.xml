<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.one.kcar.member.dao.IMemberDAO">

	<select id="isExistId" parameterType="String" resultType="int">
		SELECT count(*) FROM kcar_member WHERE m_email=#{m_email}
	</select>
	<insert id="insertMember" parameterType="MemberDTO">
		INSERT INTO kcar_member VALUES(m_id_seq.nextval, #{m_pw}, #{m_name}, #{m_email}, #{m_zipcode}, #{m_addr1}, #{m_addr2},#{m_tel})
	</insert>
	<select id="login" parameterType="MemberDTO" resultType="MemberDTO">
		SELECT * FROM kcar_member WHERE m_email=#{m_email}
	</select>

	

</mapper>